    "@simplewebauthn/browser": "^9.0.1",
    "@spotlightjs/astro": "^1.0.16",
    "@tailwindcss/vite": "next",
    "arctic": "^1.2.1",
    "astro": "4.5.2",
    "lucia": "^3.1.1",
    "lucia-adapter-astrodb": "^0.0.3",
    "oslo": "^1.1.3",
    "simple-stack-form": "^0.1.12",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "next",
    "typescript": "^5.4.2",
