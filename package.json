    "@simplewebauthn/browser": "^9.0.1",
    "@spotlightjs/astro": "^1.0.16",
    "arctic": "^1.2.1",
    "astro": "4.6.3",
    "lucia": "^3.1.1",
    "lucia-adapter-astrodb": "^0.0.8",
    "oslo": "^1.1.3",
    "simple-stack-form": "^0.1.12",
    "typescript": "^5.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
